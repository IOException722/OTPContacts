<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="code.com.otpcontact">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Example">
        <activity android:name="code.com.com.otpcontact.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="code.com.com.otpcontact.TabPager" />
        <activity android:name="code.com.com.otpcontact.MyAdapter" />
        <activity android:name="code.com.com.otpcontact.ContactInfo" />
        <activity android:name="code.com.com.otpcontact.Compose"></activity>
    </application>

</manifest>
